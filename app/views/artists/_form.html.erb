<%= form_for(@artist) do |f| %>

    <%= f.label :name %>
    <%= f.text_field :name %>
    <br>

    <%= f.label :user_name %>
    <%= f.text_field :user_name %>
    <br>

    <%= f.label :hometown %>
    <%= f.text_field :hometown %>
    <br>

    <%= f.label :password %>
    <%= f.password_field :password %>
    <br>

    <%= f.label :password_confirmation %>
    <%= f.password_field :password_confirmation %>
    <br>

    <%= f.submit %>

<% end %>